<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>My Blogs</title>
    <link rel="stylesheet" href="/styles.css" />
  </head>
  <body>
    <%- include('./partials/navbar.ejs') %> <%= console.log(blogs); %>
    <div class="blogs-list">
      <h1>All Blogs</h1>
      <% blogs.forEach((blog)=>{ %>
      <div class="each-blog">
        <h2><%= blog.title%></h2>
        <p><%= blog.snippet%></p>
      </div>
      <% })%>
    </div>
  </body>
</html>
